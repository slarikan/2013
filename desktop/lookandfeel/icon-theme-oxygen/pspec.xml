    <Source>
        <Name>icon-theme-oxygen</Name>
        <Homepage>http://www.oxygen-icons.org</Homepage>
        <Packager>
            <Name>Yusuf Aydemir</Name>
            <Email>yusuf.aydemir@istanbul.com</Email>
        </Packager>
        <License>LGPLv3+</License>
        <IsA>data</IsA>
        <Summary>Oxygen icon theme</Summary>
        <Description>oxygen-icon-theme is a high quality, photo-realistic icon theme (including both SVG and raster icons) initially created for KDE4.</Description>
        <Archive sha1sum="4034b61585db7e8c03f3270d860aa7443f1a6193" type="tarxz">mirrors://kde/stable/4.8.4/src/oxygen-icons-4.8.4.tar.xz</Archive>
    </Source>

    <Package>
        <Name>icon-theme-oxygen-png</Name>
        <Summary>Oxygen icon theme in PNG format</Summary>
        <Description>icon-theme-oxygen-png package contains Oxygen icon theme which is designed as default icon theme of KDE4 in PNG format.</Description>
        <RuntimeDependencies>
            <Dependency release="current">icon-theme-oxygen-index</Dependency>
        </RuntimeDependencies>
        <Files>
            <Path fileType="data">/usr/share/icons/oxygen/8x8</Path>
            <Path fileType="data">/usr/share/icons/oxygen/16x16</Path>
            <Path fileType="data">/usr/share/icons/oxygen/22x22</Path>
            <Path fileType="data">/usr/share/icons/oxygen/32x32</Path>
            <Path fileType="data">/usr/share/icons/oxygen/48x48</Path>
            <Path fileType="data">/usr/share/icons/oxygen/64x64</Path>
            <Path fileType="data">/usr/share/icons/oxygen/128x128</Path>
            <Path fileType="data">/usr/share/icons/oxygen/256x256</Path>
        </Files>
    </Package>

    <Package>
        <Name>icon-theme-oxygen-svg</Name>
        <Summary>Oxygen icon theme in SVG format</Summary>
        <Description>icon-theme-oxygen-svg package contains Oxygen icon theme which is designed as default icon theme of KDE4 in SVG format.</Description>
        <RuntimeDependencies>
            <Dependency release="current">icon-theme-oxygen-index</Dependency>
        </RuntimeDependencies>
        <Files>
            <Path fileType="data">/usr/share/icons/oxygen/scalable</Path>
        </Files>
    </Package>

    <Package>
        <Name>icon-theme-oxygen-index</Name>
        <Summary>Index file for Oxygen icon theme</Summary>
        <Description>icon-theme-oxygen-index package contains index.theme file for Oxygen icon theme which is a metadata file.</Description>
        <Files>
            <Path fileType="data">/usr/share/icons/oxygen/index.theme</Path>
            <Path fileType="doc">/usr/share/doc</Path>
        </Files>
    </Package>

