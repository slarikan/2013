        <Archive type="tarxz" sha1sum="5644962fcd82f8c147249507339494a697f5f695">mirrors://kde/stable/4.9.3/src/kde-l10n/kde-l10n-gl-4.9.3.tar.xz</Archive>
        <BuildDependencies>
            <Dependency versionFrom="4.9.3">kdelibs-devel</Dependency>
        </BuildDependencies>
    </Source>

    <Package>
        <Name>kde-l10n-gl</Name>
        <Files>
            <Path fileType="localedata">/usr/share/locale</Path>
        </Files>
    </Package>

    <Package>
        <Name>kde-l10n-gl-doc</Name>
        <Summary>Galician documentation files for KDE</Summary>
        <Files>
            <Path fileType="data">/usr/share/kde4</Path>
            <Path fileType="man">/usr/share/man</Path>
            <Path fileType="doc">/usr/share/doc</Path>
        </Files>
    </Package>

