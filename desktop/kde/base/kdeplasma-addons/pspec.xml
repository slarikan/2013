    <Package>
        <Name>kdeplasma-addons</Name>
        <RuntimeDependencies>
            <Dependency>qt-webkit</Dependency>
            <Dependency versionFrom="4.9.3">kde-baseapps</Dependency>
            <Dependency versionFrom="4.9.3">kde-workspace</Dependency>
